            <kind>1</kind>
            <string>if (!tutoriald) {
    if (!cantrigger) exit;
    if (!instance_exists(obj_player)) exit;
    if (!instance_exists(obj_hooktip)) exit;
    if (!obj_hooktip.grappled) exit;
    if (point_distance(obj_player.x,obj_player.y,obj_hooktip.x,obj_hooktip.y) &lt; obj_player.wb_maxdist) {
        tutoriald=true; 
        cantrigger=false; 
        visible=true;
        surf = surface_create(surface_get_width(application_surface),surface_get_height(application_surface));  
        surface_copy(surf,0,0,application_surface);
        instance_deactivate_all(true);
        instance_activate_object(obj_controller);
    }
} else {
    if (alph&lt;1) alph+=0.02;
    if (!device_mouse_check_button(0,mb_left) &amp;&amp; !keyboard_check(vk_right) &amp;&amp; !keyboard_check(vk_left) &amp;&amp; alph&gt;=1) {
        tutoriald=false;
        instance_activate_all();
        //Force boost
        with (obj_player) {
            player_release();
            player_wallboost(obj_hooktip.attachedblock, obj_hooktip.boostdir, true);
        }
        with (obj_swiper) instance_destroy();
    }
}
</string>
