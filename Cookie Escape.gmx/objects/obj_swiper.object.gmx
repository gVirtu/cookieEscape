            <string>var mx, my, ix, iy;
mx = (device_mouse_x(did)-view_xview[0]);
my = (device_mouse_y(did)-view_yview[0]);
ix = mx - lastx;
iy = my - lasty;
c_x += ix;
c_y += iy;
    //Record
    ds_queue_enqueue(swx, ix);
    ds_queue_enqueue(swy, iy);
    //Upperbound on record count
    if (ds_queue_size(swx)&gt;hist_count) {
        c_x -= ds_queue_dequeue(swx); //Remove them from cumulative dists
        c_y -= ds_queue_dequeue(swy);
    }
    
//The moment we get a long enough swipe, go
if (point_distance(0,0,c_x,c_y)&gt;swipe_dist) {  
    //show_message("C_X="+string(c_x)+"#C_Y="+string(c_y));
    var dir = point_direction(0,0,c_x,c_y);
    var swipedir=0;
    if (dir&gt;=45 &amp;&amp; dir&lt;135) {
        swipedir=90;   
    } else if (dir&gt;=135 &amp;&amp; dir&lt;225) {
        swipedir=180;
    } else if (dir&gt;=225 &amp;&amp; dir&lt;315) {
        swipedir=270;
    }
    do_swipe(swipedir);
    instance_destroy();
}
