            <string>//Configure aspect ratio

var base_w = view_wview[0];
var base_h = view_hview[0];
var aspect = base_w / base_h ; // get the GAME aspect ratio
if (display_get_gui_width() &lt; display_get_gui_height())
    {
    //portrait
    var ww = display_get_gui_width();
    var hh = ww / aspect;
    }
else
    {
    //landscape
    var hh = display_get_gui_height();
    var ww = hh * aspect;
    }
surface_resize(application_surface, ww, hh);
</string>
